                                 CLEAN_INACT:
                                 ;limpia el flag de inactivity en el acel
                                 
0000d8 ea86                      LDI R24, 0xA6		; Direccion del esclavo SLA(1010011) + Write(0)
0000d9 eac7                      LDI R28, 0xA7		; Direccion del esclavo SLA(1010011) + Read(1)
0000da e390                      LDI R25, 0x30		; Direccion del registro a leer DEVID 
0000db e0a2                      LDI R26, 0b00000010	; Dato contra el que se debe comparar la lectura
0000dc d050                      RCALL SINGLE_BYTE_READ
                                 
0000dd 17a7                      CP	R26,R23
0000de f411                      BRNE NO_INACT
                                 OK:
0000df e041                      LDI R20, 0b00000001
0000e0 b948                      OUT PORTC, R20	
                                 NO_INACT:
                                 
0000e1 9508                      RET
                                 
                                 AUX:
                                 
                                 ; Se realiza la lectura del registro DEVID que tiene una direccion: 0x00 y reset value: 11100101
0000e2 ea86                      	LDI R24, 0xA6		; Direccion del esclavo SLA(1010011) + Write(0)
0000e3 eac7                      	LDI R28, 0xA7		; Direccion del esclavo SLA(1010011) + Read(1)
                                 	;LDI R25, 0x00		; Direccion del registro a leer DEVID 
                                 	;LDI R26, 0b11100101	; Dato contra el que se debe comparar la lectura
                                 	
                                 
                                 
                                 	;The inactivity bit is set when acceleration of less than the value
                                 	;stored in the THRESH_INACT register (Address 0x25) is experienced
                                 	;for more time than is specified in the TIME_INACT
                                 	;register (Address 0x26). The maximum value for TIME_INACT
                                 	;is 255 sec. 
                                 
                                 ; Se comienza la secuencia de seteo del acelerometro en los valores necesarios
                                 
0000e4 e295                      	LDI R25, 0x25		;Direccion del registro a escribir ;THRESH_INACT 0x25 0b0010010
0000e5 e0a6                      	LDI R26, 0b00000110	;Dato a transmitir ;THRESH_INACT  8 bit unsigned. No dejar en 0x00 umbral de inactividad (62.5 mg/LSB)11111110 
                                 				;en 17 = 0b00010001 despues hacer ajuste fino
0000e6 d03d                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 
0000e7 e296                      	LDI R25, 0x26		;Direccion del registro a escribir ;TIME_INACT 0x26 0b00100110 umbral de tiempo de inactividad
0000e8 e0a2                      	LDI R26, 0b00000010	;Dato a transmitir ;TIME_INACT  (maximo 255 segundos) (1 sec/LSB)
                                 				;en 1s despues hacer ajuste fino
0000e9 d03a                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 
0000ea e297                      	LDI R25, 0x27		;Direccion del registro a escribir ;ACT_INACT_CTL 0x27 0b00100111 controla los ejes que intervienen
0000eb e0af                      	LDI R26, 0b00001111	;Dato a transmitir ;ACT_INACT_CTL 
                                 				;en 0b00000111 (D3 en 1=ac coupled)(D2D1D0 = 111 enable en los tres ejes)
0000ec d037                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 	
0000ed e29c                      	LDI R25, 0x2C		;Direccion del registro a escribir ;BW_RATE 0x2C 0b00101100 Data rate and power mode control
0000ee e0ae                      	LDI R26, 0b00001110	;Dato a transmitir ;BW_RATE
                                 				;(D4=0 sin Low Power)(Rate D3D2D1D0 = 0110 : BW=3.13Hz) 
0000ef d034                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 
0000f0 e29f                      	LDI R25, 0x2F		;Direccion del registro a escribir ;INT_MAP 0x2F 0b00101111 Interrupt mapping control
0000f1 efa7                      	LDI R26, 0b11110111	;Dato a transmitir ;INT_MAP
                                 				;solo la inactividad conectada al pin INT1, el resto conectadas al pin INT2
0000f2 d031                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 
0000f3 e29e                      	LDI R25, 0x2E		;Direccion del registro a escribir ;INT_MAP 0x2E 0b00101110 Interrupt enable control
0000f4 e0a8                      	LDI R26, 0b00001000	;Dato a transmitir ;INT_ENABLE
                                 				;se habilita solo la interrupcion por inactividad
0000f5 d02e                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 
                                 ; Solo queda la duda de si en Reg 0x2D POWER_CTL hay que poner el (Measure)D3 en 1 para que anden las interrupciones o no
                                 ; Por defecto esta en 0 al encenderse en modo standby
                                 
0000f6 e29d                      	LDI R25, 0x2D	;Direccion del registro a escribir ;0x2D POWER_CTL 0b00101101 
0000f7 e0a8                      	LDI R26, 0b00001000	;Dato a transmitir ; MEASURE ON
                                 				;se pone en 1 el bit de Measure, por defecto se prende en modo standby, no queda claro si es necesario
                                 				;ponerlo en modo medicion para que anden las interrupciones
0000f8 d02b                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
0000f9 9508                      RET
                                 
