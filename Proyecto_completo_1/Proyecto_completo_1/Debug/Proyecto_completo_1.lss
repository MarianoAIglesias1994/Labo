                                 CONFIG_ACELEROMETRO:
                                 
                                 ;Conexiones hardware:
                                 ;Arduino Pin	ADXL345 Pin		Placa CdR Atmega88pa-pu
                                 ;GND				GND				8,22
                                 ;3V3				VCC				7,20
                                 ;3V3				CS				
                                 ;GND				SDO				
                                 ;A4					SDA				27
                                 ;A5					SCL				28
                                 ;					INT1			4
                                 
                                 ;Informacion de la datasheet del ADXL345
                                 
                                 
                                 
                                 ; Se realiza la lectura del registro DEVID que tiene una direccion: 0x00 y reset value: 11100101
00001c ea86                      	LDI R24, 0xA6		; Direccion del esclavo SLA(1010011) + Write(0)
00001d eac7                      	LDI R28, 0xA7		; Direccion del esclavo SLA(1010011) + Read(1)
                                 	;LDI R25, 0x00		; Direccion del registro a leer DEVID 
                                 	;LDI R26, 0b11100101	; Dato contra el que se debe comparar la lectura
                                 	
                                 
                                 
                                 	;The inactivity bit is set when acceleration of less than the value
                                 	;stored in the THRESH_INACT register (Address 0x25) is experienced
                                 	;for more time than is specified in the TIME_INACT
                                 	;register (Address 0x26). The maximum value for TIME_INACT
                                 	;is 255 sec. 
                                 
                                 ; Se comienza la secuencia de seteo del acelerometro en los valores necesarios
                                 
00001e e295                      	LDI R25, 0x25		;Direccion del registro a escribir ;THRESH_INACT 0x25 0b0010010
00001f e0a1                      	LDI R26, 0b00000001	;Dato a transmitir ;THRESH_INACT  8 bit unsigned. No dejar en 0x00 umbral de inactividad (62.5 mg/LSB) 
                                 				;en 17 = 0b00010001 despues hacer ajuste fino
000020 d103                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 
000021 e296                      	LDI R25, 0x26		;Direccion del registro a escribir ;TIME_INACT 0x26 0b00100110 umbral de tiempo de inactividad
000022 e0a2                      	LDI R26, 0b00000010	;Dato a transmitir ;TIME_INACT  (maximo 255 segundos) (1 sec/LSB)
                                 				;en 1s despues hacer ajuste fino
000023 d100                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 
000024 e297                      	LDI R25, 0x27		;Direccion del registro a escribir ;ACT_INACT_CTL 0x27 0b00100111 controla los ejes que intervienen
000025 e0af                      	LDI R26, 0b00001111	;Dato a transmitir ;ACT_INACT_CTL 
                                 				;en 0b00000111 (D3 en 1=ac coupled)(D2D1D0 = 111 enable en los tres ejes)
000026 d0fd                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 	
000027 e29c                      	LDI R25, 0x2C		;Direccion del registro a escribir ;BW_RATE 0x2C 0b00101100 Data rate and power mode control
000028 e0a6                      	LDI R26, 0b00000110	;Dato a transmitir ;BW_RATE
                                 				;(D4=0 sin Low Power)(Rate D3D2D1D0 = 0110 : BW=3.13Hz) 
000029 d0fa                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 
00002a e29f                      	LDI R25, 0x2F		;Direccion del registro a escribir ;INT_MAP 0x2F 0b00101111 Interrupt mapping control
00002b efa7                      	LDI R26, 0b11110111	;Dato a transmitir ;INT_MAP
                                 				;solo la inactividad conectada al pin INT1, el resto conectadas al pin INT2
00002c d0f7                      	RCALL MULTIPLE_BYTE_WRITE
                                 ;	RCALL DELAY
                                 
                                 
00002d e29e                      	LDI R25, 0x2E		;Direccion del registro a escribir ;INT_MAP 0x2E 0b00101110 Interrupt enable control
00002e e0a8                      	LDI R26, 0b00001000	;Dato a transmitir ;INT_ENABLE
                                 				;se habilita solo la interrupcion por inactividad
