                                 /**************************************************************
                                 CONFIGURACION I2C
                                 ***************************************************************/
                                 
                                 
                                 CONFIG_I2C:
                                 
                                 I2C_INIT:
0000fa e050                      	LDI R21, 0		
0000fb 9350 00b9                 	STS TWSR, R21		; Preescaler 1 en TWI Status Reg
0000fd eb50                      	LDI R21, 0xB0		;  0xB0
0000fe 9350 00b8                 	STS TWBR, R21		; Setea la frecuencia a 50.087 kHz (18.432 MHz XTAL)
000100 e054                      	LDI R21, (1<<TWEN)	; 0x04 a R21 (TWEN: Enable bit)
000101 9350 00bc                 	STS TWCR, R21		; Habilita el TWI 
000103 9508                      	RET
                                 
                                 	
                                 
                                 ; TWINT:TWI Interrupt Flag 
                                 ; TWSTA:TWI Start condition bit
                                 
                                 I2C_START:
000104 ea54                      	LDI R21, (1<<TWINT)|(1<<TWSTA)|(1<<TWEN);
000105 9350 00bc                 	STS TWCR, R21		; Transimitir condicion de START
                                 	
                                 WAIT1:
000107 9150 00bc                 	LDS R21, TWCR		; Lee el registro
000109 ff57                      	SBRS R21, TWINT		; Saltea siguiente linea si TWINT es 1(==operacion finalizada)
00010a cffc                      	RJMP WAIT1			; TWINT esta en 0
00010b 9508                      	RET
                                 
                                 
                                 
                                 ; Se debe cargar el dato a enviar en el registro TWDR (R27)
                                 ; TWDR: TWI Data Register
                                 
                                 I2C_WRITE:
00010c 93b0 00bb                 	STS TWDR, R27		; Lleva el byte a TWDR
00010e e854                      	LDI R21, (1<<TWINT)|(1<<TWEN) ; Se setean TWINT y TWEN en el TWCR
00010f 9350 00bc                 	STS TWCR, R21		; Configura TWCR para enviar TWDR
                                 
                                 WAIT2:
000111 9150 00bc                 	LDS R21, TWCR		; Lee el registro de control a R21
000113 ff57                      	SBRS R21, TWINT		; Saltea siguiente liÂ­nea si TWINT es 1
000114 cffc                      	RJMP WAIT2			; TWINT esta en 0
000115 9508                      	RET
                                 
                                 
                                 I2C_READ:
000116 e854                      	LDI R21, (1<<TWINT)|(1<<TWEN)
000117 9350 00bc                 	STS TWCR, R21
                                 
                                 WAIT3:
000119 9150 00bc                 	LDS R21, TWCR
00011b ff57                      	SBRS R21, TWINT
00011c cffc                      	RJMP WAIT3
00011d 91b0 00bb                 	LDS R27, TWDR		; Guarda en R27 el dato leido
00011f 9508                      	RET
                                 
                                 
                                 
                                 ;TWSTO:TWI Stop condition bit
                                 
                                 I2C_STOP:
000120 e954                      	LDI R21, (1<<TWINT)|(1<<TWSTO)|(1<<TWEN)
000121 9350 00bc                 	STS TWCR, R21		; Transmitir condicion de STOP
000123 9508                      	RET
                                 
                                 
                                 
                                 
                                 ; Subrutina de escritura de registros del acelerometro 
                                 MULTIPLE_BYTE_WRITE:		
000124 dfdf                      	RCALL I2C_START		; Transmite la condicion de START
                                 	
000125 2fb8                      	MOV R27, R24		; Carga la direccion del esclavo + configuracion W
000126 dfe5                      	RCALL I2C_WRITE		; Escribe R27 al bus I2C
                                 	
000127 2fb9                      	MOV R27, R25		; Direccion del registro a escribir
000128 dfe3                      	RCALL I2C_WRITE		; Escribe R27 al bus I2C
                                 	
000129 2fba                      	MOV R27, R26		; Dato a transmitir 
00012a dfe1                      	RCALL I2C_WRITE		; Escribe R27 al bus I2C
                                 
00012b dff4                      	RCALL I2C_STOP 		;Transmite la condicion de STOP
00012c 9508                      	RET
                                 
                                 
                                 ; Subrutina de lectura de registros del acelerometro 
                                 SINGLE_BYTE_READ:
00012d dfd6                      	RCALL I2C_START		; Transmite la condicion de START
                                 
00012e 2fb8                      	MOV R27, R24		; Carga la direccion del esclavo + configuracion W
00012f dfdc                      	RCALL I2C_WRITE		; Escribe R27 al bus I2C
                                 
000130 2fb9                      	MOV R27, R25		; Direccion del registro a leer
000131 dfda                      	RCALL I2C_WRITE		; Escribe R27 al bus I2C
                                 
                                 
000132 dfd1                      	RCALL I2C_START		; Retransmite Start
                                 
                                 
000133 2fbc                      	MOV R27, R28		; Carga la direccion del esclavo + configuracion R
000134 dfd7                      	RCALL I2C_WRITE
                                 
000135 e0b0                      	LDI R27,0
                                 
000136 dfdf                      	RCALL I2C_READ
000137 2f7b                      	MOV R23, R27
                                 
000138 dfe7                      	RCALL I2C_STOP
000139 9508                      	RET
                                 
                                 
                                 
                                 
