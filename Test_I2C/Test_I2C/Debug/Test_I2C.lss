                                 ; Biblioteca I2C
                                 
                                 /**************************************************************
                                 CONFIGURACION I2C
                                 ***************************************************************/
                                 CONFIG_I2C:
                                 
                                 I2C_INIT:
000018 e050                      	LDI R21, 0		
000019 9350 00b9                 	STS TWSR, R21		; Preescaler 1 en TWI Status Reg
00001b e554                      	LDI R21, 0x54		;  0xB0
00001c 9350 00b8                 	STS TWBR, R21		; Setea la frecuencia a 50.087 kHz (18.432 MHz XTAL)
                                 ;	LDI R21, (1<<TWEN)	; 0x04 a R21 (TWEN: Enable bit)
                                 ;	STS TWCR, R21		; Habilita el TWI 
00001e 9508                      	RET
                                 
                                 	
                                 
                                 ; TWINT:TWI Interrupt Flag 
                                 ; TWSTA:TWI Start condition bit
                                 
                                 I2C_START:
00001f ea54                      	LDI R21, (1<<TWINT)|(1<<TWSTA)|(1<<TWEN);
000020 9350 00bc                 	STS TWCR, R21		; Transimitir condicion de START
                                 	
                                 WAIT1:
000022 9150 00bc                 	LDS R21, TWCR		; Lee el registro
000024 ff57                      	SBRS R21, TWINT		; Saltea siguiente linea si TWINT es 1(==operacion finalizada)
000025 cffc                      	RJMP WAIT1			; TWINT esta en 0
000026 9508                      	RET
                                 
                                 
                                 
                                 ; Se debe cargar el dato a enviar en el registro TWDR (R27)
                                 ; TWDR: TWI Data Register
                                 
                                 I2C_WRITE:
000027 93b0 00bb                 	STS TWDR, R27		; Lleva el byte a TWDR
000029 e854                      	LDI R21, (1<<TWINT)|(1<<TWEN) ; Se setean TWINT y TWEN en el TWCR
00002a 9350 00bc                 	STS TWCR, R21		; Configura TWCR para enviar TWDR
                                 
                                 WAIT2:
00002c 9150 00bc                 	LDS R21, TWCR		; Lee el registro de control a R21
00002e ff57                      	SBRS R21, TWINT		; Saltea siguiente liÂ­nea si TWINT es 1
00002f cffc                      	RJMP WAIT2			; TWINT esta en 0
000030 9508                      	RET
                                 
                                 
                                 I2C_READ:
000031 e854                      	LDI R21, (1<<TWINT)|(1<<TWEN)
000032 9350 00bc                 	STS TWCR, R21
                                 
                                 WAIT3:
000034 9150 00bc                 	LDS R21, TWCR
000036 ff57                      	SBRS R21, TWINT
000037 cff4                      	RJMP WAIT2
000038 91b0 00bb                 	LDS R27, TWDR		; Guarda en R27 el dato leido
00003a 9508                      	RET
                                 
                                 
                                 
                                 ;TWSTO:TWI Stop condition bit
                                 
                                 I2C_STOP:
00003b e954                      	LDI R21, (1<<TWINT)|(1<<TWSTO)|(1<<TWEN)
00003c 9350 00bc                 	STS TWCR, R21		; Transmitir condicion de STOP
00003e 9508                      	RET
                                 
                                 
                                 ; Check value of TWI status register. Mask prescaler bits. If
                                 ; status different from START go to ERROR
                                 
                                 CHECK_TWI_ST_REG:
00003f 9150 00b9                 	LDS R21,TWSR		; TWSR: TWI status register
000041 7f58                      	ANDI R21, 0xF8		; Mascara
000042 1751                      	CP R21, R17			; El status se pasa por R17
000043 f409                      	BRNE ERROR
000044 c003                      	RJMP NO_ERROR
                                 ERROR:
000045 e040                      	LDI R20, 0b00000000	; En caso de error del status se prenden los LEDs Verdes
000046 b94b                      	OUT PORTD, R20		
000047 9508                      	RET
                                 NO_ERROR:
000048 9508                      	RET
                                 
                                 /*
                                 ; Subrutina de escritura de registros del acelerometro 
                                 MULTIPLE_BYTE_WRITE:		
                                 	RCALL I2C_START		; Transmite la condicion de START
                                 	LDI R17, 0x10		; 0x08: A START condition has been transmitted
                                 						; 0x10: A repeated START condition has been transmitted
                                 	RCALL CHECK_TWI_ST_REG; Chequea que este OK el status reg del TWI, recibe previamente el status en R17
                                 	
                                 	MOV R27, R24		; Carga la direccion del esclavo + configuracion W
                                 	RCALL I2C_WRITE		; Escribe R27 al bus I2C
                                 	LDI R17, 0x18		; 0x18: SLA+W has been transmitted ;ACK has been received
                                 	RCALL CHECK_TWI_ST_REG;
                                 	
                                 	MOV R27, R25		; Direccion del registro a escribir
                                 	RCALL I2C_WRITE		; Escribe R27 al bus I2C
                                 	LDI R17, 0x28		; 0x28: Data byte has been transmitted; ACK has been received
                                 	RCALL CHECK_TWI_ST_REG;
                                 	
                                 	MOV R27, R26		; Dato a transmitir 
                                 	RCALL I2C_WRITE		; Escribe R27 al bus I2C
                                 	LDI R17, 0x28		; 0x28: Data byte has been transmitted; ACK has been received
                                 	RCALL CHECK_TWI_ST_REG;
                                 
                                 	RCALL I2C_STOP 		;Transmite la condicion de STOP
                                 	RET
                                 */
                                 
                                 
                                 ; Subrutina de lectura de registros del acelerometro 
                                 SINGLE_BYTE_READ:
000049 dfd5                      	RCALL I2C_START		; Transmite la condicion de START
00004a d02b                      	RCALL DELAY
00004b d02a                      	RCALL DELAY
00004c d029                      	RCALL DELAY
00004d e018                      	LDI R17, 0x08		;  0x08: A START condition has been transmitted
00004e dff0                      	RCALL CHECK_TWI_ST_REG; Chequea que este OK el status reg del TWI, recibe previamente el status en R17
                                 	
                                 
00004f 2fb8                      	MOV R27, R24		; Carga la direccion del esclavo + configuracion W
000050 dfd6                      	RCALL I2C_WRITE		; Escribe R27 al bus I2C
000051 d024                      	RCALL DELAY
000052 d023                      	RCALL DELAY
000053 d022                      	RCALL DELAY
000054 e118                      	LDI R17, 0x18		; 0x18: SLA+W has been transmitted ;ACK has been received
000055 dfe9                      	RCALL CHECK_TWI_ST_REG;
                                 
                                 
000056 2fb9                      	MOV R27, R25		; Direccion del registro a escribir
000057 dfcf                      	RCALL I2C_WRITE		; Escribe R27 al bus I2C
000058 d01d                      	RCALL DELAY
000059 d01c                      	RCALL DELAY
00005a d01b                      	RCALL DELAY
00005b e218                      	LDI R17, 0x28		; 0x28: Data byte has been transmitted; ACK has been received
